// This is a test program which can be compiled by the TastierCompiler.
// It reads a sequence of numbers from the input file and computes, for each
// number, the sum of all integers up to that number
 
program Test {
     

   
    Struct structure{
     int i[5][3][2];
     string stringArray[2][4]
    }
    int arr2[3];

 void procP(Struct structure n){
    write n.i[2][1][1];
    write n.stringArray[2][2];// printing hello
    write n.stringArray[1][1];//printing weee....
    n.stringArray[2][2]:="hi";
  }
  void procP2(){
     arr2[0]:=5;
     arr2[1]:=2;
     arr2[2]:=7;

  }

  void Main() { 
    
      
   int  arr [5][3][2];
    arr[2][1][1]:=2321;


    string  st1 [6][3];
    st1[2][2]:="hello";  
 
    
    Struct structw := new structure (arr,st1);
    structw.stringArray[1][1]:= "weeeeeeeeeeeeeeeeeee"; 
    procP(structw);
    write   structw.stringArray[2][2];// printing h
    
    int i;
    i:=0;
    int count;
    count:=0;

    procP2();
    for(i:=0;i:=i+1;i<3){
       count:= count+arr2[i];
    }
    write count;// prints out 14
 

  }
}